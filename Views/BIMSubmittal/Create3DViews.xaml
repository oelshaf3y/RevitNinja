<Window x:Class="Revit_Ninja.Views.BIMSubmittal.Create3DViews"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Revit_Ninja.Views.BIMSubmittal"
        mc:Ignorable="d"
        Title="Create 3D Views" Height="386" Width="309">
    <Grid>
        <Label Content="Enter View Name" HorizontalAlignment="Left" Margin="13,10,0,0" VerticalAlignment="Top"/>
        <DataGrid x:Name="dgv" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,41,0,139"/>
        <Button Content="Save" Margin="199,0,0,45" VerticalAlignment="Bottom" BorderBrush="{x:Null}" Background="#FFAEC08F" Height="26" Click="saveViewNames" HorizontalAlignment="Left" Width="42" IsDefault="True"/>
        <Button Content="Cancel" Margin="63,0,0,45" BorderBrush="{x:Null}" Background="#FFD49797" Click="cancel" Height="26" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="56" IsCancel="True"/>

    </Grid>
</Window>
